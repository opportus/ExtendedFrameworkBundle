{
    "name": "opportus/extended-framework-bundle",
    "description": "This bundle extends the Symfony framework by providing features genericizing homogeneously common development processes in order to build faster solidly architectured Symfony applications.",
    "keywords": ["symfony", "bundle", "framework", "controller", "annotations", "param converter", "entity gateway", "template engine"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Cl√©ment Cazaud",
            "email": "opportus@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.0",
        "symfony/framework-bundle": "^4.2",
        "symfony/validator": "^4.2",
        "symfony/serializer": "^4.2",
        "symfony/security-bundle": "^4.2",
        "doctrine/annotations": "^1.0",
        "opportus/object-mapper-bundle": "^1.0"
    },
    "autoload": {
        "psr-4": {"Opportus\\ExtendedFrameworkBundle\\": ""}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
